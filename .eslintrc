{
  "extends": ["eslint:recommended"],
  "rules": {
    "no-irregular-whitespace": "off",
    "no-mixed-spaces-and-tabs": "off",
    "no-console": "warn",
    "quotes": ["error", "single"],
    "array-bracket-newline": ["error", "consistent"],
    "brace-style": "error",
    "camelcase": "error",
    "comma-dangle": "error",
    "comma-spacing": ["error", { "before": false, "after": true }],
    "comma-style": ["error", "last"],
    "computed-property-spacing": ["error", "never"],
    "func-call-spacing": ["error", "never"],
    "function-paren-newline": ["error", "consistent"],
    "indent": ["error", 4, { "FunctionDeclaration": {"parameters": "first"}, "MemberExpression": 1 }],
    "key-spacing": ["error", { "beforeColon": false, "afterColon": true, "mode": "strict" }],
    "keyword-spacing": ["error", { "before": true, "after": true }],
    "max-len": ["error", 120],
    "new-parens": "error",
    "newline-per-chained-call": ["error", { "ignoreChainWithDepth": 2 }],
    "no-lonely-if": "error",
    "no-multiple-empty-lines": ["error", { "max": 1 }],
    "no-negated-condition": "error",
    "no-new-object": "error",
    "no-tabs": "error",
    "no-trailing-spaces": ["error", { "ignoreComments": true }],
    "no-unneeded-ternary": "error",
    "no-whitespace-before-property": "error",
    "curly": ["error", "all"],
    "object-curly-newline": ["error", { "minProperties": 1 }],
    "object-curly-spacing": "error",
    "object-property-newline": "error",
    "quote-props": ["error", "as-needed"],
    "semi": "error",
    "semi-style": ["error", "last"],
    "space-before-blocks": "error",
    "space-before-function-paren": ["error", "never"],
    "space-in-parens": "error",
    "switch-colon-spacing": "error",
    "eqeqeq": "error",
    "space-infix-ops": "error",
    "line-comment-position": ["error", { "position": "above", "applyDefaultIgnorePatterns": false }],
    "complexity": ["error", 10],
    "no-else-return": "error",
    "jasmine/no-focused-tests": "error",
    "jasmine/expect-matcher": "error",
    "jasmine/expect-single-argument": "error",
    "jasmine/missing-expect": ["error", "expect()", "$httpBackend.expectGET()", "$httpBackend.expectDELETE()",
      "$httpBackend.expectGET()", "$httpBackend.expectJSONP()", "$httpBackend.expectHEAD()",
      "$httpBackend.expectPATCH()", "$httpBackend.expectPOST()", "$httpBackend.expectPUT()"
    ],
    "jasmine/new-line-before-expect": "error",
    "jasmine/new-line-between-declarations": "error",
    "jasmine/no-disabled-tests": "error",
    "jasmine/no-global-setup": "error",
    "jasmine/no-spec-dupes": [2, "branch"],
    "jasmine/no-suite-dupes": [2, "branch"],
    "jasmine/no-suite-callback-args": "error",
    "jasmine/prefer-jasmine-matcher": "error"
  },
  "env": {
    "browser": true,
    "jasmine": true,
    "jquery": true
  },
  "globals": {
    "angular": true,
    "FormData": true,
    "_": true,
    "inject": true,
    "module": true,
    "Set": true
  },
  "plugins": [
    "dollar-sign",
    "jquery",
    "jasmine"
  ]
}