{
  "build": "./build",
  "serve":{
    "dev": "./build",
    "prod": "./build/webapp"
  },
  "app": {
    "src": "./src",
    "dest": "./build/webapp",
    "tmp": "./.tmp"
  },
  "styleguide": {
  	"src": "./src",
  	"dest": "./build/styleguide",
    "title": "OpenLMIS-UI Styleguide"
  },
  "docs": {
    "src": "./docs",
    "dest": "./build/docs",
    "title": "OpenLMIS-UI Documentation"
  }
}
